
cd ${DEV_DIR};
services=`./${DEV_SELF} config --services`;

user_id=`id -u`;
group_id=`id -g`;

if [ -n "${user_id}" ]; then
    userarg='--build-arg=UID='${user_id};
fi
if [ -n "${group_id}" ]; then
    grouparg='--build-arg=GID='${group_id};
fi

dc yml_all;
dc mode build;
dc cmd "${userarg} ${grouparg} $@";

for a in ${services}; do
    cmd="`dc show` $a";
    ${cmd};
done
exit;

