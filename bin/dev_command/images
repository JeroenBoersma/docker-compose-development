
dc mode config;

{ dc run; cat ${DEV_WORKPATH}/build/*/*/Dockerfile; } | \
    grep -hE '(image:|FROM |FROM_TAG:)' | \
    grep -v FROM_IMAGE | \
    sed \
        -e 's#FROM_TAG: #srcoder/development-php:#' \
        -e 's#image:##' \
        -e 's#FROM ##' \
        -e 's/$/:latest/' \
        -e 's/\(:[^:]*\):latest$/\1/' | \
    awk '{print $1}' | \
    sort | \
    uniq;
exit;

